{
  "linuxFxVersion": "DOCKER|${CONTAINER_IMAGE}",
  "appCommandLine": "",
  "alwaysOn": true,
  "http20Enabled": true,
  "httpLoggingEnabled": true,
  "logsDirectorySizeLimit": 35,
  "detailedErrorLoggingEnabled": true,
